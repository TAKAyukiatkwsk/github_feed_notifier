{
  "manifest_version": 2,
  "name": "GitHub feed notifier",
  "version": "0.0.1",

  "description": "Notify GitHub user private feed",
  "icons": {},

  "browser_action": {
    "default_popup": "popup.html"
  },

  "author": "Takayuki TAKAGI (TAKAyuki_atkwsk)",
  "background": {
    "scripts": [
      "javascripts/jquery-2.0.3.min.js",
      "javascripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "https://github.com/"
  ]
}
